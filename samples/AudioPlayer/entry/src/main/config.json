{
  "app": {
    "bundleName": "com.waylau.hmos.audioplayer",
    "vendor": "waylau",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 3,
      "target": 3
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.waylau.hmos.audioplayer",
    "name": ".MyApplication",
    "deviceType": [
      "car","tv"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "landscape",
        "name": "com.waylau.hmos.audioplayer.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "AudioPlayer",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar",
          "extra": ""
        }
      ]
    }
  }
}